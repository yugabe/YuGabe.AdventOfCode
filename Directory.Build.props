<Project>

  <PropertyGroup Condition="'$(ProjectName)' != 'Tidy.AdventOfCode'">
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <CheckForOverflowUnderflow>true</CheckForOverflowUnderflow>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup Condition="'$(ProjectName)' != 'Tidy.AdventOfCode'">
    <ProjectReference Condition="'$(ProjectName)' != 'YuGabe.AdventOfCode.Common'" Include="..\YuGabe.AdventOfCode.Common\YuGabe.AdventOfCode.Common.csproj" />
    <Using Include="Tidy.AdventOfCode"/>
  </ItemGroup>

</Project>
